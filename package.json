{
  "name": "adapt-authoring-coursetheme",
  "version": "1.2.1",
  "description": "Module for managing course theming",
  "homepage": "https://github.com/adapt-security/adapt-authoring-coursetheme",
  "license": "GPL-3.0",
  "type": "module",
  "main": "index.js",
  "repository": "github:adapt-security/adapt-authoring-coursetheme",
  "dependencies": {
    "adapt-authoring-api": "^1.3.6",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "adapt-authoring-adaptframework": "^1.9.3",
    "adapt-authoring-assets": "^1.3.1",
    "adapt-authoring-content": "^1.2.3",
    "adapt-authoring-core": "^2.0.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "conventional-changelog-eslint": "^6.0.0",
    "semantic-release": "^25.0.2",
    "standard": "^17.1.0"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "eslint"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "eslint"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "Chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "scripts": {
    "test": "node --test 'tests/**/*.spec.js'"
  }
}
